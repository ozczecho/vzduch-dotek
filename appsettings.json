{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "restrictedToMinimumLevel": "Verbose",
                "path": "Logs/vzduch.dotek.log",
                "fileSizeLimitBytes": 1024000,
                "rollOnFileSizeLimit": true,
                "retainedFileCountLimit": 5
              }
            }
          ]
        }
      }
    ]
  },
  "allowedHosts": "*",
  "vzduchPort": 5353,
  "airTouch": {
    "localHost": "192.168.1.21",
    "localPort": 8899
  }
}
