{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [{
      "Name": "Async",
      "Args": {
        "configure": [{
          "Name": "File",
          "Args": {
            "restrictedToMinimumLevel": "Verbose",
            "path": "Logs/vzduch.dotek.log",
            "rollingInterval": "Day",
            "fileSizeLimitBytes": 1024000,
            "rollOnFileSizeLimit": true,
            "retainedFileCountLimit": 10
          }
        }]
      }
    }]
  },
  "allowedHosts": "*",
  "airTouch": {
    "localHost": "192.168.80.21",
    "localPort": 8899
  }
}